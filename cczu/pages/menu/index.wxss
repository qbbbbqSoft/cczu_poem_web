
.panel {
  display: block;
  margin-top: 10rpx;
  font-size: 12px
}

.panel .chart-with-img {
  display: inline-block;
  width: 15%;
  margin: 10rpx 4%;
  text-align: center;
}

.chart-with-img image {
  box-sizing: border-box;
  width: 100%;
  height: 80rpx; 
  padding: 20rpx;
  background-color: #fff;
  border: 1rpx solid #eee;
  display: block;
  margin: 0 auto;
  margin-bottom: 10rpx;
  border-radius: 100rpx
}

.chart-without-img {
  display: block;
  width: 92%;
  margin: 10px 4%;
  border: none;
  border-radius: 0;
}

.chart-without-img button {
  text-align: left;
}
.buttom{
  width: 60rpx;
  height: 60rpx;
  background: #fff;
  display: flex;
  /* flex-direction: row; */
  position: fixed;
  bottom: 40rpx;
  right: 20rpx;
  font-size: 25rpx;
  border-radius: 60rpx;
}
.link {
  background: #fff;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 10rpx;
  right: 30rpx;
  font-size: 25rpx;
}



