@import "../common/mainTabCell.wxss";
.scrollView{
  height: 100%;
  background-color: #eaeaea;
}

.add-wrap {
  position:fixed;
  bottom:0;
  display:flex;
  width:100%;
  justify-content:center;
}
.add-wrap image {
  border: 2px solid #ccc;
  width:80rpx;
  height:80rpx;
  border-radius: 80rpx;
}

.buttom{
  width: 80rpx;
  height: 80rpx;
  background: #fff;
  display: flex;
  position: fixed;
  bottom: 40rpx;
  right: 20rpx;
  font-size: 25rpx;
  border-radius: 60rpx;
}
.search {
  width: 80rpx;
  height: 80rpx;
  background: #fff;
  display: flex;
  position: fixed;
  bottom: 140rpx;
  right: 20rpx;
  font-size: 25rpx;
  border-radius: 60rpx;
}
.search_area {
  display: inline-flex;
  position: relative;
  margin-left: 20rpx; 
}
.search_text {
  display: flex;
  word-break:break-all;
}
.search_button {
  display: flex;
  margin-left:20rpx;float:right;
}

#box {
    width: 100%;
    height: 300px;
    background: #43b0d1;
    position: relative;
}
#child {
    width: 100%;
    height: 100px;
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    line-height: 100px;
    text-align: center;
}